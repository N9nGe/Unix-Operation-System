// Used for CP2

#define CAPSLOCK_PRESSED 0x3A
#define CAPSLOCK_RELEASED 0xBA
#define ALT_PRESSED 0x38
#define ALT_RELEASED 0xB8
#define F1  0x3B
#define F2  0x3C
#define F3  0x3D
#define F4  0x3E
#define F5  0x3F
#define ESC 0x01
#define TAB_PRESSED 0x0F
#define UP_PRESSED 0x48
#define DOWN_PRESSED 0X50

